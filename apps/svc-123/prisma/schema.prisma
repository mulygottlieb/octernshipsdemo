datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Po3aorder {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  t1        String?
  t2        String?
  t3        String?
  updatedAt DateTime @updatedAt
}

model OfStore {
  createdAt DateTime @default(now())
  fld1      String?
  fld2      String?
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}
